    "inquirer": "^8.1.5",
    "minimatch": "^3.0.4",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.31",
    "@olad/eslint-config-ts": "workspace:*",
    "@olad/eslint-config-basic": "workspace:*",
    "@olad/eslint-config": "workspace:*",
