{
  "name": "@olad/auto-lint",
  "version": "2.0.8",
  "description": "CLI 命令行工具",
  "author": "Zhaoxm",
  "main": "./build",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc --build ./tsconfig.json"
